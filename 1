class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        int m,n;
        for(int i=0;i<nums.size();i++){
            int x,y;
            x=nums[i];
            y=target-x;
            for(int j=i+1;j<nums.size();j++){
                if(nums[j]==y){
                    m=i;
                    n=j;
                    break;
                }
            }
        }
        vector <int> ans(2);
        ans[0]=m;
        ans[1]=n;
        return ans;
    }
};
