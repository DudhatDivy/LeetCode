class Solution {
public:
    bool isPalindrome(int x) {
        if(x<0){
            return 0;
        }
        else{
              long long int m=x;
              long long int sum=0;
                while(m!=0){
                    sum=sum*10+(m%10);
                    m=m/10;
                }
                int k=0;
                if(sum==x){
                    k=1;
                }
                return k;
        }
    }
};
